{
  "name": "google-optimize-react-ssr-sample",
  "description": "Google OptimizeをReactのSSR時に仕込むサンプル",
  "version": "0.1.0",
  "author": "Himenon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "yarn clean && run-p webpack:build tsc:build",
    "develop": "run-p tsc:build:watch develop:server",
    "develop:server": "nodemon --watch lib --watch dist/manifest.json --exec 'node ./lib/server/server.js'",
    "tsc:build": "tsc -p tsconfig.json",
    "tsc:build:watch": "yarn tsc:build --watch",
    "docker:build": "node ./scripts/docker-build.js",
    "docker:server": "node ./scripts/docker-server.js",
    "webpack:build": "webpack --config ./webpack.config.ts"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "log4js": "6.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/copy-webpack-plugin": "6.0.0",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/gtag.js": "0.0.3",
    "@types/jest": "26.0.14",
    "@types/log4js": "2.3.5",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/webpack": "4.41.22",
    "@types/webpack-manifest-plugin": "2.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.2.1",
    "core-js": "3.6.5",
    "fork-ts-checker-notifier-webpack-plugin": "3.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "jest": "26.5.3",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "regenerator-runtime": "0.13.7",
    "resolve-pkg": "2.0.0",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "4.0.0",
    "webpack-manifest-plugin": "2.2.0"
  }
}
